        }

            .card-group.card-group-scroll > .card {
                flex-basis: 35%;
            }

        ::-webkit-scrollbar {
            width: 30px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            border-radius: 5px;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: #E7E7E7;
